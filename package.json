{
  "name": "quality-assurance",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:single-spa",
    "build": "npm run build:single-spa",
    "test": "ng test",
    "e2e": "ng e2e",
    "prodBuild": "ng build --configuration production --output-hashing none",
    "qaBuild": "ng build --configuration qa --output-hashing none",
    "devBuild": "ng build --configuration dev --output-hashing none",
    "build:single-spa": "ng build quality-assurance --prod --deploy-url /dist/quality-assurance --output-hashing none",
    "serve:single-spa": "ng serve --disable-host-check --port 4206 --deploy-url http://localhost:4206/ --live-reload false",
    "lint": "eslint ./"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^8",
