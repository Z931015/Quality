    "msal": "^1.4.4",
    "ng2-pdf-viewer": "^7.0.1",
    "ngx-logger": "^4.1.9",
    "pipes-module": "^1.0.1",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^11.4.0",
    "rxjs": "~6.6.3",
    "single-spa": "^5.5.0",
    "single-spa-angular": "^4.0.1",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
